decross:
  paths:
    input: /path/to/folder/with/data/
    output: /path/to/output/folder

  hits_filtering:
    len_ratio: 70
    len_minimum: 100

  coverage_ratio:
    regular: 1.5
    close: 0.04
    left_eats_right: 10
    right_eats_left: 0.1

  threads:
    multithreading:  false
    count:   1
